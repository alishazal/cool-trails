{
    "functions": {
      "api/**/*.py": {
        "runtime": "python3.9.6"  
      }
    },
    "build": {
      // this runs BEFORE 'pip install -r requirements.txt'
      "installCommand":
        "curl -L -o data/trails.db https://github.com/rlmoore-b/cool-trails/releases/download/v1.0.0/trails.db && pip install -r requirements.txt"
    }
  }
  